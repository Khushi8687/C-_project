﻿@model C__project.Models.StaffDto
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<form method="POST" action="/Staff/edit/@Model.StaffID">
    <input type="text" style="display:none;" name="StaffId" value="@Model.StaffID" />
    <div>
        <label>Name</label>
        <input type="text" name="AnimalName" value="@Model.StaffName" />
    </div>
    <div>
        <label>Email</label>
        <input type="email" name="Email" value="@Model.Email" />
    </div>
    <div>

        <input type="submit" value="Update" />
    </div>

</form>